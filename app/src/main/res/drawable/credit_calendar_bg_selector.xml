<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:state_selected="true">
        <shape android:shape="oval">
            <solid android:color="@color/white_90"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="@color/white_90"/>
        </shape>
    </item>
    <item app:tsquare_state_current_month="false" android:drawable="@android:color/transparent"/>
    <item app:tsquare_state_highlighted="true" android:drawable="@drawable/credit_calendar_due_date_circle"/>
    <item app:tsquare_state_today="true" android:drawable="@android:color/transparent"/>
    <item android:drawable="@android:color/transparent"/>
</selector>